{
  "manifest_version": 2,
  "name": "JIRA Ticket Opener",
  "description": "Opens a JIRA ticket of your choosing!",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Navigate to your tickets!"
  },
  "options_page": "options.html",
  "permissions": ["storage"]
}
